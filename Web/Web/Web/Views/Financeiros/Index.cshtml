@model IEnumerable<Web.Models.Financeiro>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DRegistro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorVenda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorDespesa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorLucro)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaFim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorVenda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorDespesa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorLucro)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
